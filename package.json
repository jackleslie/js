{
  "name": "project-name",
  "private": true,
  "version": "0.0.0",
  "main": "dist/main.js",
  "module": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "build": "babel src -o dist/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": {
    "name": "Jack Leslie",
    "email": "jackleslie@protonmail.com",
    "url": "https://jackleslie.dev"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "babel-jest": "^26.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e -V",
      "pre-push": "jest"
    }
  },
  "lint-staged": {
    "{__tests__,src}/**/*.js": [
      "eslint --fix --quiet",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
